@model IEnumerable<UpskillingMVCWebApp.Models.ProjectDto>

<div class="dropdown">
    <button class="btn dropdown-toggle" type="button" id="dropdownMenuButton1" data-bs-toggle="dropdown" aria-expanded="false">
        Projects
    </button>
    <ul class="dropdown-menu" aria-labelledby="dropdownMenuButton1">
        <li><a class="dropdown-item nav-link text-dark" asp-area="" asp-controller="Projects" asp-action="Index">All projects</a></li>
        @foreach (ProjectDto project in Model)
        {
            <li><a class="dropdown-item nav-link text-dark" asp-area="" asp-controller="Projects" asp-action="Details" asp-route-id="@project.ProjectId">@project.Name</a></li>
        }
    </ul>
</div>
